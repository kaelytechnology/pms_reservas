{
    "name": "kaelytechnology/pms_hotel",
    "description": "Hotel Property Management System package for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kaely Technology",
            "email": "info@kaelytechnology.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Kaely\\PmsHotel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaely\\PmsHotel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaely\\PmsHotel\\PmsHotelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}